{
  "name": "intoTask",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "electron": "electron .",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "package": "",
    "build": "webpack-cli && electron-packager . intoTask && cp ./assets/intoTask_logo_main_A_512px_default.icns intoTask-darwin-x64/intoTask.app/Contents/Resources/electron.icns && electron-packager . intoTask --platform=win32"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-stage-1": "^6.1.18",
    "electron": "^2.0.0",
    "electron-packager": "^12.0.2",
    "lodash": "^3.10.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-redux": "5.0.7",
    "react-router": "^2.0.1",
    "react-sparklines": "^1.7.0",
    "redux": "4.0.0",
    "redux-promise": "^0.5.3"
  }
}
